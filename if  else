#include<stdio.h>
int main()
{
	int age = 100;
	if (age < 18)
		printf("未成年\n");
	else if (age >= 18 && age < 50)
		printf("壮年\n");
	else if(age >= 50 && age < 90)
		printf("老年\n");
	else
		printf("6\n");
	return 0;
}




#include<stdio.h>
int main()
{
	int num = 4;
	if (5 == num)
	{
		printf("hehe\n");
	}
	return 0;
}
